{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "types": [
      "node",
      "@wdio/globals/types",
      "@types/jasmine",
      "expect-webdriverio/jasmine",
      "@wdio/visual-service",
      "expect-webdriverio/jasmine-wdio-expect-async"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
